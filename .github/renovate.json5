{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": [
    "custom.regex",
    "github-actions",
    "pre-commit",
  ],
  "extends": [
    "config:recommended",
    ":automergeStableNonMajor",
    ":enablePreCommit",
    ":semanticCommitTypeAll(fix)",
    "helpers:pinGitHubActionDigests",
    "regexManagers:githubActionsVersions",
  ],
  "platformAutomerge": false,
}
